{
    "manifest_version": 2,
    "version": "0.1",
    "name": "Z Score",
    "description": "Quickly search normal distribution z-score and proportion from chrome.",

    "author": "https://github.com/joeybuddy",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "main.html"
    },
    "options_page": "options.html",
    "options_ui": {
        "chrome_style": true,
        "page": "options.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "short_name": "zscore",
    "storage": {
        "state_data": "state_data.json"
    }
}